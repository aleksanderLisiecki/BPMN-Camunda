      <bpmn:script>var cena = execution.getVariable('cenaTowaru');
var ilosc = execution.getVariable('liczbaSztuk');
var kosztPrzesylki = execution.getVariable('kosztPrzesylki');

var sladWeglowy = 0;

if(execution.getVariable('sladWeglowy') != null){
